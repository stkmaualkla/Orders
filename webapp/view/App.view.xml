	<App id="app" busy="{appView>/busy}" busyIndicatorDelay="{appView>/delay}">
		<pages>
			<Page xmlns="sap.m" id="pageId" title="INTERNATIONAL EXPRESS ORDERS" floatingFooter="true" backgroundDesign="Transparent" titleLevel="H1" icon="sap-icon://flight" showFooter="false">
